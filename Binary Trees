#BINARY TREES
class BinaryTree:
	def __init__(self):
		self.root=None
		self.size=0
	def length(self):
		return self.size
	def __iter__(self):
		return self.root.__iter__()

class TreeNode:
	def __init__(self,key, value, left=None, right=None, parent=None):
		self.key=key
		self.value=value
		self.leftC=left #left child
		self.rightC=right # right child
		self.parent=parent
	
